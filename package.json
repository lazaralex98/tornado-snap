{
  "name": "tornado-snap",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "description": "A dapp/snap to store tornado cash notes",
  "scripts": {
    "build:snap": "yarn workspace snap build:clean",
    "build:next": "yarn workspace next build",
    "start:snap": "yarn workspace snap serve",
    "start:next": "yarn workspace next dev",
    "build": "yarn build:snap && yarn build:next"
  },
